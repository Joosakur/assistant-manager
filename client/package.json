{
  "name": "assistant-manager-client",
  "version": "0.1.0",
  "description": "Client side of a web-application for planning, tracking and reporting personal assistant working hours.",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "eslint src/**",
    "lintfix": "eslint src/** --fix",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Joosa Kurvinen",
  "license": "MIT",
  "dependencies": {
    "amplifyjs": "1.1.2-beta.2",
    "axios": "0.17.1",
    "babel-preset-stage-0": "6.24.1",
    "clipboard": "1.7.1",
    "history": "4.7.2",
    "moment": "2.20.1",
    "object-assign": "4.1.1",
    "query-string": "6.1.0",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-big-calendar": "0.17.0",
    "react-color": "2.13.8",
    "react-datetime": "2.12.0",
    "react-dom": "16.2.0",
    "react-localization": "1.0.10",
    "react-recaptcha": "2.3.6",
    "react-redux": "5.0.6",
    "react-redux-toastr": "7.2.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-auth-wrapper": "2.0.2",
    "redux-form": "6.8.0",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "semantic-ui-react": "0.77.2"
  },
  "devDependencies": {
    "autoprefixer": "7.2.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "browser-sync": "2.23.6",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.3.1",
    "coveralls": "2.11.15",
    "css-loader": "0.28.9",
    "enzyme": "3.3.0",
    "eslint": "4.16.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.7.0",
    "eslint-plugin-react": "7.6.0",
    "eslint-watch": "3.1.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.1.4",
    "json-loader": "0.5.7",
    "mockdate": "2.0.2",
    "nock": "9.2.5",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.0.10",
    "prompt": "1.0.0",
    "prop-types": "15.6.0",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "15.5.4",
    "redux-immutable-state-invariant": "2.1.0",
    "replace": "0.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "sinon": "5.0.10",
    "style-loader": "0.20.1",
    "sugarss": "1.0.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.2",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.0",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
    },
    "globals": {
      "__API_ORIGIN__": "http://localhost:9000",
      "__SELF_ORIGIN__": "http://localhost:9100"
    }
  }
}
